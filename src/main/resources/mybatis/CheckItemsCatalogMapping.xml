<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gzjy.checkitems.mapper.CheckItemsCatalogMappingMapper">
	

  <resultMap id="BaseResultMap" type="com.gzjy.checkitems.model.CheckItemsCatalogMapping">
    <id column="id" jdbcType="VARCHAR" property="Id" />    
    <result column="catalog_id" jdbcType="VARCHAR" property="catalogId" />  
    <result column="check_item_id" jdbcType="VARCHAR" property="checkItemId" />  
    <result column="laboratory" jdbcType="VARCHAR" property="laboratory" />  
  </resultMap>  
  
  <sql id="Base_Column_List">
    id, catalog_id, check_item_id, laboratory
  </sql>
  
  <select id="selectByPrimaryKey" parameterType="java.lang.String" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />   
    from check_item_catalog_mapping
    where id = #{id,jdbcType=VARCHAR}
  </select>     
  

</mapper>